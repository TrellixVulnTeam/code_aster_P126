TEST_TABLE(CRITERE='RELATIF',REFERENCE='SOURCE_EXTERNE',PRECISION=0.39, VALE_CALC=0.0002420532087124187, VALE_REFE=0.0002178030303030304, NOM_PARA='DX',TABLE=TAB,FILTRE=_F(NOM_PARA='INST',VALE=8.500E-3,),)

