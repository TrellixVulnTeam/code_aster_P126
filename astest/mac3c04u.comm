TEST_TABLE(CRITERE='RELATIF',REFERENCE='SOURCE_EXTERNE',PRECISION=0.03, VALE_CALC=0.0004955461875463126, VALE_REFE=0.0005025826442263548, NOM_PARA='DX',TABLE=TAB,FILTRE=_F(NOM_PARA='INST',VALE=7.500E-4,),)
